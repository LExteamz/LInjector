<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:shell="clr-namespace:System.Windows.Shell;assembly=PresentationFramework"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:local="clr-namespace:LInjector"
    xmlns:controls="clr-namespace:LInjector.Pages"
    xmlns:custom="clr-namespace:LInjector.Pages"
    xmlns:Wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
    x:Class="LInjector.Windows.MainWindow"
    mc:Ignorable="d"
    MinHeight="400"
    MinWidth="600"
    Height="400" Width="600" ShowInTaskbar="True" WindowStartupLocation="CenterScreen"
    Icon="\LInjector.ico"
    ResizeMode="CanResize"
    WindowStyle="None"
    AllowsTransparency="True"
    Background="Transparent"
    SourceInitialized="Window_SourceInitialized"
    Loaded="Window_Loaded">

    <shell:WindowChrome.WindowChrome>
        <shell:WindowChrome CaptionHeight="0" UseAeroCaptionButtons="True" NonClientFrameEdges="None" ResizeBorderThickness="10" />
    </shell:WindowChrome.WindowChrome>

    <Grid Margin="10">
        <controls:MainView x:Name="MainViewz" Visibility="Visible"/>
    </Grid>
</Window>
